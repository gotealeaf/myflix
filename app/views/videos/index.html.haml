%article.video_category
  - @categories.each do |category|
    %header
      %br
        %h3= link_to category.name, category_path(category)
      
    .videos.row
      %br
        - 4.times do
          - @videos.each do |video|
            .video.col-sm-2
              = link_to video, nil do
                %img(src="#{video.small_cover_url}")



