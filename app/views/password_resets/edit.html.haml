%section.reset_password.container
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_for(@user, url: password_reset_path(params[:id])) do |f|
        %header
          %h1 Enter your new password
        %fieldset.form-group
          = f.password_field :password
          = f.submit("Update Password")