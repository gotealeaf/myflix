%section.sign_in.container
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_tag(url: sessions_path, method: :post, class:'sign_in') do |f|
        %header
          %h1 Sign in
        .row
          .col-sm-5
            = f.email_field :email
            = f.password_field :password
            = f.submit "Sign In"
