%section.register.container
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_for(@user, layout: :horizontal, label_col: "col-sm-2", control_col: "col-sm-4") do |f|
        %header
          %h1 Register
        = f.email_field :email
        = f.password_field :password
        = f.text_field :full_name
        = f.form_group do
          = f.submit
