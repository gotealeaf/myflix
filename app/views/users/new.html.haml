%section.register.container
  .row
    .col-sm-10.col-sm-offset-1
      / = render 'shared/errors', obj: @user
      = bootstrap_form_for(@user, url: register_path) do |f|
        %header
          %h1 Register
        %fieldset
          = f.email_field :email
          = f.text_field :fullname, label: "Full Name"
          = f.password_field :password
          = f.submit("Submit")
