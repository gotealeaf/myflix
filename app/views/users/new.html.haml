%section.register.container
  .row
    .col-sm-10.col-sm-offset-1
      %form.form-horizontal
        %header
          %h1 Register
          = bootstrap_form_for(@user) do |f|
            = f.email_field :email
            = f.password_field :password
            = f.text_field :full_name
            = f.submit
